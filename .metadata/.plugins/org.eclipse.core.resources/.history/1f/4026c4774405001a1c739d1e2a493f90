

import java.util.Scanner;

public class Prob01 {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		System.out.print("단 수 입력: ");
		int dan = scanner.nextInt();
		int a = 0;
		
		  
		  while (a < 9) { 
			  a++; 
			  System.out.println(dan + " x " + a + " = " + a * dan);
		  
		  }
		 
		 

		scanner.close();

	}// main
}// class
