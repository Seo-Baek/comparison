package prexercise01;

public class ProInBook5_5For {

	public static void main(String[] args) {
		// 1부터 10까지 곱해서 출력
		int mul = 1;
		
		for(int num = 1; num < 11; num++) {
			mul *= num;
			
		}
		System.out.println("1부터 10까지의 곱: " + mul);
		
		
		// 구구단 중 5단을 출력하는 프로그램
		// for문 사용
		int i = 5;
		for (int j = 1; j < 10; j++) {
			System.out.println(i + " X " + j + " = " + i * j);
			
		}
	
			
		
		
		

	}//main

}//class
