package practice01;

public class Prob05 {

	public static void main(String[] args) {
		
		
		int number = 10000;
		String strNumber = String.valueOf(number);
		
		for(int i = 0; i <= number; i++) {
			char c = strNumber.charAt(i); 
			char clapCount = '0';
			if(c == '3' || c == '6' || c =='9') {
				clapCount++;
				System.out.println(c + "Â¦");
			}
			
		}
		
		
	}

}
