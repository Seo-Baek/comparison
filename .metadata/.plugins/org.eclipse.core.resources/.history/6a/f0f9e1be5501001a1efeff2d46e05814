package chapter01;

public class SumInput {

	public static void main(String[] args) {
		int number = 10;
		int sum = 0;
		
		//?
		
		
		while(sum < number) {
			
			sum = sum + (sum + 1);
		
			System.out.println(number + "까지 합은" + sum + "입니다.");
			sum = sum + 1;
		}
		


	}

}
