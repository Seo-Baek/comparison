package prexercise01;

public class Takoyakki {

	public static void main(String[] args) {
		//구구단을 외자
		//구구단을 하되 단이 끝날 때마다 구분선을 예!쁘!게! 그어주자 ^0^
		
		int dan = 2;
		int su	= 1;
		
		while (dan < 10) {
			System.out.println(dan + "단");
			System.out.println(su);
			while (su < 10) {
				System.out.println(dan + " x " + su + " = " + dan * su);
				su++;
				
			}
			System.out.println("**********");
			dan++;
			
		}
		

	}//main

}//class
